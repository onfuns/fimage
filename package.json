{
  "name": "fimage",
  "version": "1.2.0",
  "author": "onfuns",
  "main": "./main.js",
  "description": "Fimage by onfuns",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint {src,main,scripts}/**/*.{js,jsx,ts,tsx}",
    "start": "cross-env NODE_ENV=development webpack --config ./scripts/webpack.dev.config.js",
    "electron": "npm run build:main && cross-env NODE_ENV=development electron .",
    "build:renderer": "rimraf ./renderer_process && cross-env NODE_ENV=production webpack -p --progress --color --config ./scripts/webpack.prod.config.js",
    "build:main": "rimraf ./main_process && babel ./main --out-dir ./main_process",
    "build": "npm run build:main && npm run build:renderer",
    "package:mac": "npm run build && electron-builder --mac",
    "package:win": "npm run build && electron-builder --win --ia32 ",
    "package:linux": "npm run build && electron-builder --linux",
    "package:all": "npm run build && electron-builder -mwl",
    "pretty": "prettier --write '{src,main,scripts}/**/*.{jsx,tsx,ts,js,json,css,html}'"
  },
  "prettier": "@frontend-cli/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "keywords": [
    "electron app",
    "electron react",
    "electron compress"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@frontend-cli/prettier-config": "^1.0.3",
    "antd": "^3.10.8",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "electron": "9.4.0",
    "electron-builder": "^21.2.0",
    "electron-reload": "^1.3.0",
    "eslint": "^3.17.1",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^19.0.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mini-css-extract-plugin": "^0.6.0",
    "mobx": "^5.15.1",
    "mobx-react": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-loadable": "^5.5.0",
    "react-router": "^4.3.1",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.3",
    "style-loader": "^0.18.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "fs-extra": "^7.0.1",
    "imagemin": "^6.1.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-jpegoptim": "^6.0.0",
    "imagemin-optipng": "^6.0.0",
    "imagemin-pngquant": "^7.0.0",
    "imagemin-svgo": "^7.0.0"
  },
  "build": {
    "appId": "Fimage",
    "productName": "Fimage",
    "artifactName": "${productName}.${ext}",
    "asar": false,
    "directories": {
      "output": "packages",
      "buildResources": "resources"
    },
    "files": [
      "resources",
      "main_process",
      "renderer_process",
      "main.js",
      "package.json"
    ],
    "mac": {
      "target": "dmg",
      "icon": "resources/icon.icns"
    },
    "dmg": {
      "contents": [
        {
          "type": "file",
          "x": 130,
          "y": 150
        },
        {
          "type": "link",
          "path": "/Applications",
          "x": 410,
          "y": 150
        }
      ]
    }
  },
  "win": {
    "target": "nsis",
    "icon": "resources/icon.ico"
  },
  "nsis": {
    "oneClick": false,
    "allowElevation": true,
    "allowToChangeInstallationDirectory": true,
    "installerIcon": "resources/icon.ico",
    "uninstallerIcon": "resources/icon.ico",
    "installerHeaderIcon": "resources/icon.ico",
    "createDesktopShortcut": true,
    "createStartMenuShortcut": true
  }
}
